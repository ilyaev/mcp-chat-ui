    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@openai/agents": "^0.0.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.8",
    "chrono-node": "^2.8.3",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "google-auth-library": "^10.1.0",
    "jsonwebtoken": "^9.0.2",
    "math-expression-evaluator": "^2.0.7",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "object-path": "^0.11.8",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "ws": "^8.18.3",
    "zod": "<=3.25.67"
  },
  "devDependencies": {
