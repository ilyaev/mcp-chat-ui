    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/object-path": "^0.11.4",
    "concurrently": "^9.2.0",
    "jest": "^30.0.3",
    "nodemon": "^3.1.10",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.0"
  }
}